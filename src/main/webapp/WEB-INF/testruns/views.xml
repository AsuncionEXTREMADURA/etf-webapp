<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC
	"-//Apache Software Foundation//DTD Tiles Configuration 2.1//EN"
	"http://tiles.apache.org/dtds/tiles-config_2_1.dtd">
	
<tiles-definitions>

	<definition name="testruns/*" extends="standardLayout" templateType="thymeleaf">
		<put-attribute name="body" value="/testruns/{1}.html :: content" type="thymeleaf"/>
	</definition>
	
	<definition name="testruns/monitor" extends="bodyOnly" templateType="thymeleaf">
		<put-attribute name="body" value="/testruns/monitor.html :: content" type="thymeleaf"/>
	</definition>

</tiles-definitions>
